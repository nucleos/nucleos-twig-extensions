{
    "name": "nucleos/dev-tools",
    "description": "Development tools that do not conflict the project dependencies",
    "type": "project",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "maglnet/composer-require-checker": "^4.6",
        "matthiasnoback/symfony-dependency-injection-test": "^5.0.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.15",
        "phpstan/phpstan-phpunit": "^1.3.13",
        "phpstan/phpstan-strict-rules": "^1.5.1",
        "phpstan/phpstan-symfony": "^1.3.2",
        "phpunit/phpunit": "^10.1.3",
        "psalm/plugin-phpunit": "^0.18.4",
        "psalm/plugin-symfony": "^5.0.3",
        "symfony/phpunit-bridge": "^7.0.0",
        "vimeo/psalm": "^5.12"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "bin-dir": "../../vendor/bin"
    },
    "extra": {
        "symfony": {
            "require": "6.4.*"
        }
    }
}
